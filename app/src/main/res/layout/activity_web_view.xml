<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <com.ys.pa200.weight.HeadColumnView
        android:id="@+id/webview_headColumnView"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        app:is_show_left_btn="true"
        app:is_show_right_text="false"
        app:title_context="公司简介" />

    <com.ys.pa200.weight.ProgressWebView
        android:id="@+id/adv_jump"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none" />
</LinearLayout>